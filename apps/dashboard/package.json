{
  "name": "dashboard",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "graphql-codegen --config ./introspection.yml",
    "dev": "concurrently --kill-others --names \"client_vite,client_codegen\" --prefix-colors \"magenta,gray\" \"npm run dev:host\" \"npm run codegen:graphql:watch\"",
    "dev:host": "vite --host --port 3001",
    "start": "vite --port 3001",
    "prebuild": "pnpm run codegen:graphql",
    "build": "vite build && tsc",
    "serve": "vite preview --port 3001",
    "test": "vitest run",
    "codegen:graphql": "graphql-codegen",
    "codegen:graphql:watch": "graphql-codegen --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@repo/ui": "workspace:*",
    "@repo/validation-schema": "workspace:*",
    "@tanstack/react-router": "^1.114.3",
    "@tanstack/react-router-devtools": "^1.114.3",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/router-plugin": "^1.114.3",
    "better-auth": "^1.2.7",
    "date-fns": "^4.1.0",
    "graphql": "^16.11.0",
    "lucide-react": "^0.475.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.2",
    "sonner": "^2.0.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.8.1",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-apollo-client-helpers": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.5.1",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/vite": "^4.1.5",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "jsdom": "^26.0.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.5",
    "tsup": "^8.4.0",
    "typescript": "^5.7.2",
    "vite": "^6.1.0",
    "vitest": "^3.0.5",
    "web-vitals": "^4.2.4"
  },
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=10.0.0"
  }
}
